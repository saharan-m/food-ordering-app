als.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Meals/AvailableMeals.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Meals/MealsSummary.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/UI/Card.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Meals/MealItem/MealItem.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Meals/MealItem/MealItemForm.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/UI/Input.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Cart/Cart.js",["78"],"import React, { useContext, useState } from \"react\";\nimport classes from \"./Cart.Module.css\";\nimport Modal from \"../UI/Modal\";\nimport CartContext from \"../../store/cart-context\";\nimport CartItem from \"./CartItem\";\nimport Checkout from \"./Checkout\";\nconst Cart = (props) => {\n  const [isCheckout, setIsCheckout] = useState(false);\n  const [isSubmitting, setIsSubmitting] = useState(false);\n  const [isSubmitted, setIsSubmitted] = useState(false);\n  const [isError, setIsError] = useState();\n  const cartCtx = useContext(CartContext);\n  const totalAmount = `$${cartCtx.amount.toFixed(2)}`;\n  const hasItems = cartCtx.items.length > 0;\n\n  const cartItemRemoveHandler = (id) => {\n    cartCtx.removeItem(id);\n  };\n  const cartItemAddHandler = (item) => {\n    cartCtx.addItem({ ...item, amount: 1 });\n  };\n  const orderHandler = () => {\n    setIsCheckout(true);\n  };\n  const submitOrderHandler = async (userData) => {\n    try {\n      setIsSubmitting(true);\n      const response = await fetch(\n        \"https://react-8fdd2-default-rtdb.asia-southeast1.firebasedatabase.app/orders.json\",\n        {\n          method: \"POST\",\n          body: JSON.stringify({\n            user: userData,\n            orderedItems: cartCtx.items,\n          }),\n        }\n      );\n      if (!response.ok) {\n        throw new Error(\"Something went wrong oops!\");\n      }\n      setIsSubmitted(true);\n      setIsSubmitting(false);\n      cartCtx.clearCart();\n    } catch (error) {\n      setIsSubmitting(false);\n      setIsError(error.message);\n    }\n  };\n  const cartItems = (\n    <ul className={classes[\"cart-items\"]}>\n      {cartCtx.items.map((item) => (\n        <CartItem\n          id={item.id}\n          key={item.id + Math.random()}\n          name={item.name}\n          amount={item.amount}\n          price={item.price}\n          onAdd={cartItemAddHandler.bind(null, item)}\n          onRemove={cartItemRemoveHandler.bind(null, item.id)}\n        />\n      ))}\n    </ul>\n  );\n  const modalActions = (\n    <div className={classes.actions}>\n      <button className={classes[\"button--alt\"]} onClick={props.onHideCart}>\n        Close\n      </button>\n      {hasItems && (\n        <button className={classes.button} onClick={orderHandler}>\n          Order\n        </button>\n      )}\n    </div>\n  );\n  const modalContent = (\n    <React.Fragment>\n      {cartItems}\n      <div className={classes.total}>\n        <span>Total Amount</span>\n        <span>{totalAmount}</span>\n      </div>\n      {isCheckout && (\n        <Checkout onConfirm={submitOrderHandler} onCancel={props.onHideCart} />\n      )}\n      {!isCheckout && modalActions}\n    </React.Fragment>\n  );\n  const isSubmittingModalContent = (\n    <p>We are submitting your order please be patient!</p>\n  );\n  const didSubmitModalContent = (\n    <React.Fragment>\n      <p>Order Successfully placed!</p>\n      <div className={classes.actions}>\n      <button className={classes.button} onClick={props.onHideCart}>\n        Close\n      </button>\n    </div>\n    </React.Fragment>\n  );\n  return (\n    <Modal onClose={props.onHideCart}>\n      {!isSubmitting && !isSubmitted && modalContent}\n      {isSubmitting && isSubmittingModalContent}\n      {!isSubmitting && isSubmitted && didSubmitModalContent}\n    </Modal>\n  );\n};\n\nexport default Cart;\n","/home/mayank/Projects/foodapp/Food Ordering App/src/components/UI/Modal.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/store/CartProvider.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/store/cart-context.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Cart/CartItem.js",[],"/home/mayank/Projects/foodapp/Food Ordering App/src/components/Cart/Checkout.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":11,"column":10,"nodeType":"85","messageId":"86","endLine":11,"endColumn":17},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'isError' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]